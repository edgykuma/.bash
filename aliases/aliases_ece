#Aliases used for 240 onward
alias vcs-sv='vcs -q -sverilog'
alias vcs-sv-urg='vcs-sv -cm line -cm_line contassign'
alias urg-run='urg -dir simv.vdb -format text'
alias vlogan-sv='vlogan -sverilog -nc'
alias vcs-mod='vcs-sv -nc'

alias update-all='sudo apt update && sudo apt upgrade && sudo apt autoremove'
alias view='vim -R'

alias howbig='du -h -d 1'

alias x11?='echo $DISPLAY'

alias hopen='handin240_open'
alias hclose='handin240_close'
alias hextend='handin240_extend'
